---
async function fetchCats() {
    const response = await fetch("https://freetestapi.com/api/v1/cats");
    const data = await response.json();
    return data;
}

const cats = await fetchCats();
---

<div>
    <h1>List of Cats</h1>
    <ul>
        {cats.map((cat) => <li key={cat.id}>{cat.name}</li>)}
    </ul>
</div>
